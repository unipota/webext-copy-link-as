{
  "name": "copy-link-as",
  "productName": "CopyLinkAs",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "license": "UNLICENSED",
  "private": true,
  "scripts": {
    "dev": "yarn dev:firefox",
    "dev:firefox": "cross-env BROWSER_TARGET='firefox-desktop' yarn watch",
    "dev:firefox-android": "cross-env BROWSER_TARGET='firefox-android' yarn watch",
    "dev:chromium": "cross-env BROWSER_TARGET='chromium' yarn watch",
    "watch": "cross-env NODE_ENV='development' webpack -w --config webpack.config.js",
    "build": "yarn build:firefox",
    "build:firefox": "cross-env BROWSER_TARGET='firefox-desktop' yarn prod",
    "build:firefox-android": "cross-env BROWSER_TARGET='firefox-android' yarn prod",
    "build:chromium": "cross-env BROWSER_TARGET='chromium' yarn prod",
    "prod": "cross-env NODE_ENV='production' webpack --config webpack.config.js",
    "lint": "web-ext lint --source-dir ./dist/"
  },
  "dependencies": {
    "@typescript-eslint/eslint-plugin": "^4.22.0",
    "@typescript-eslint/parser": "^4.22.0",
    "comment-json": "^4.1.0",
    "copy-webpack-plugin": "^8.1.1",
    "cross-env": "^7.0.3",
    "css-minimizer-webpack-plugin": "^2.0.0",
    "mini-css-extract-plugin": "^1.6.0",
    "ts-loader": "^9.1.1",
    "typescript": "^4.2.4",
    "web-ext": "^6.1.0",
    "webextension-polyfill-ts": "^0.25.0",
    "webpack": "^5.35.1",
    "webpack-cli": "^4.6.0"
  }
}
